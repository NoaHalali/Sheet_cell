<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="115.0" prefWidth="179.0" spacing="5.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="components.left.commands.graph.GraphDialogController">
    <Label prefHeight="17.0" prefWidth="145.0" text="Select Columns for Graph:" />

    <HBox spacing="5.0">
        <Label text="X Axis" />
        <ComboBox fx:id="xAxisComboBox" />
      <VBox.margin>
         <Insets />
      </VBox.margin>
    </HBox>

    <HBox spacing="5.0">
        <Label text="Y Axis" />
        <ComboBox fx:id="yAxisComboBox" />
      <VBox.margin>
         <Insets />
      </VBox.margin>
    </HBox>

    <Button onAction="#handleCreateGraph" text="Create" />
   <padding>
      <Insets left="5.0" />
   </padding>
</VBox>
